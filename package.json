{
  "name": "autoassist_backend",
  "version": "1.0.0",
  "description": "Backend API for AutoAssist Car Marketplace",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "import-data": "node src/scripts/import-cars.js",
    "create-sample": "node create-sample-data.js",
    "db:setup": "node -e \"require('./src/config/database').initDatabase().then(() => process.exit(0)).catch(err => { console.error(err); process.exit(1); })\"",
    "db:reset": "node -e \"require('./src/config/database').resetDatabase().then(() => process.exit(0)).catch(err => { console.error(err); process.exit(1); })\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "car",
    "marketplace",
    "api",
    "backend",
    "express",
    "mongodb"
  ],
  "author": "AutoAssist Team",
  "license": "ISC",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.7.1",
    "morgan": "^1.10.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
